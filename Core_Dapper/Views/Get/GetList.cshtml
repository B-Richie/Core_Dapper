@model IEnumerable<IArtistDto>
@{
    ViewData["Title"] = "GetList";
}

<h2>GetList</h2>

<div>
    <div>
        <a href="/Home/Index" class="btn btn-default">Back</a>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Album ID
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var artist in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(m => artist.ArtistID)
                    </td>
                    <td>
                        @Html.DisplayFor(m => artist.ArtistName)
                    </td>                    
                </tr>
            }

        </tbody>
    </table>
</div>

