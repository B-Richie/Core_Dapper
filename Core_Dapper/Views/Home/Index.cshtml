
@{
    ViewData["Title"] = "Home Page";
}

<style>
    .dropbtn {
        background-color: #31b0d5;
        border-color: #269abc;
        color: white;
        padding: 16px;
        font-size: 16px;
        border: none;
        cursor: pointer;
    }

    .dropdown {
        position: relative;
        display: inline-block;
        padding: 15px 15px;
    }

    .dropdown-content {
        display: none;
        position: absolute;
        background-color: #f9f9f9;
        min-width: 200px;
        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
        z-index: 1;
    }

        .dropdown-content a {
            color: black;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
        }

            .dropdown-content a:hover {
                background-color: #f1f1f1
            }

    .dropdown:hover .dropdown-content {
        display: block;
    }

    .dropdown:hover .dropbtn {
        background-color: #269abc;
    }
</style>


<div class="dropdown">
    <button class="dropbtn">Get</button>
    <div class="dropdown-content">
        <a href="/Get/GetList">Get List</a>
        <a href="/Get/GetListAsync">Get List Async</a>
        <a href="/Get/GetFirstOrDefault">Get First or Default</a>
        <a href="/Get/GetFirstOrDefaultAsync">Get First or Default Async</a>
        <a href="/Get/GetOverride">Get Override</a>
    </div>
</div>

<div class="dropdown">
    <button class="dropbtn">Insert</button>
    <div class="dropdown-content">
        <a href="/Insert/Index">Insert</a>
        @*<a href="/Insert/Index">Insert Async</a>*@
    </div>
</div>

<div class="dropdown">
    <button class="dropbtn">Update</button>
    <div class="dropdown-content">
        <a href="/Update/Index">Update</a>
        @*<a href="#">Update Async</a>*@
    </div>
</div>

<div class="dropdown">
    <button class="dropbtn">Delete</button>
    <div class="dropdown-content">
        <a href="/Delete/Index">Delete</a>
        @*<a href="#">Delete Async</a>*@
    </div>
</div>

<div class="dropdown">
    <button class="dropbtn">Execute Procedure</button>
    <div class="dropdown-content">
        <a href="/ExecuteProc/Execute">Execute</a>
        <a href="/ExecuteProc/ExecuteAsync">Execute Async</a>
    </div>
</div>

<div class="dropdown">
    <button class="dropbtn">Transaction</button>
    <div class="dropdown-content">
        <a href="/Transaction/Index">Transaction</a>
    </div>
</div>

